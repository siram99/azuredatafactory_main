{
	"name": "dataflow7",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "source_employee",
						"type": "DatasetReference"
					},
					"name": "EMPLOYE1"
				},
				{
					"dataset": {
						"referenceName": "source_employee2",
						"type": "DatasetReference"
					},
					"name": "EMPLOYE22"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Sinkemployee",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "union1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empid as string,",
				"          firstname as string,",
				"          lastname as string,",
				"          age as string,",
				"          salary as string,",
				"          gender as string,",
				"          deptno as string,",
				"          state as string,",
				"          mobilenumber as string,",
				"          mail as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EMPLOYE1",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EMPLOYE22",
				"EMPLOYE1, EMPLOYE22 union(byName: true)~> union1",
				"union1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['union_employee.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}